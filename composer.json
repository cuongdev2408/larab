{
    "name": "cuongdev/larab",
    "description": "CuongDev Laravel Abstraction for starter project",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CuongDev",
            "email": "cuongnv.developer@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^v8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
        "prettus/l5-repository": "^2.9.0",
        "tymon/jwt-auth": "^2.0.0",
        "spatie/laravel-permission": "^6.9.0"
    },
    "autoload": {
        "psr-4": {
            "CuongDev\\Larab\\": "./src/",
            "CuongDev\\Larab\\Database\\Factories\\": "./src/database/factories/",
            "CuongDev\\Larab\\Database\\Seeders\\": "./src/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CuongDev\\Larab\\LarabServiceProvider",
                "Spatie\\Permission\\PermissionServiceProvider"
            ]
        }
    }
}
